{
    "name": "geangeowle/chronos-php",
    "description": "Very Small Lite Framework for PHP",
    "keywords": [
        "chronosPHP",
        "framework"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gean Geowle",
            "email": "geangeowle@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Jônatan Flôres",
            "email": "jonatafloress@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
      "psr-4": {
        "Chronos\\": "src"
      },
      "files": [
        "src/requirements.php",
        "src/functions.php",
        "src/Config/boot.php"
      ]
    },
    "autoload-dev": {
      "psr-4": {
          "Chronos\\Test\\": "tests"
      }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.5",
        "phpunit/phpunit": "^5.7",
        "php-coveralls/php-coveralls": "^2.0"
    }
}
